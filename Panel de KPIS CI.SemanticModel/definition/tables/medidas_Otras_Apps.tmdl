	measure 'Rank OtrasApps con filtro fecha' =
			
			if(ISBLANK(SUMX(RELATEDTABLE(Otras_Apps_Hecho), Otras_Apps_Hecho[visitas totales])),
			BLANK(),
			RANKX(
			    FILTER(ALLSELECTED(Otras_Apps_Hecho),medidas_Otras_Apps[Numero eventos Apps]>0),Otras_Apps_Hecho[visitas totales],
			    SUMX(RELATEDTABLE(Otras_Apps_Hecho), Otras_Apps_Hecho[visitas totales])
			))
