	measure 'Comparision Usuarios Potenciales Apps' =
			
			VAR TipoEpoca = Comun[Tipo Epoca]
			VAR AnoEpoca = Comun[Año Epoca] -1
			VAR Subepoca = Comun[Sub Epoca numero]
			VAR denominator = CALCULATE(
			    SUM(App_Campus_Accesos_Hecho[usuarios potenciales]),
			    FILTER(
			        'App_Campus_Accesos_Hecho',
			        'App_Campus_Accesos_Hecho'[Tipo Epoca] = TipoEpoca &&
			        'App_Campus_Accesos_Hecho'[Año] = AnoEpoca &&
			        (
			            (TipoEpoca = "Anual") ||
			            (TipoEpoca = "Mensual" && 'App_Campus_Accesos_Hecho'[Mes] = Subepoca) ||
			            (TipoEpoca = "Semestral" && 'App_Campus_Accesos_Hecho'[Semestre] = Subepoca)
			        )
			    )
			)
			
			VAR numerator = medidas_apps[Usuarios Potenciales App]
			VAR difference = IF(ISBLANK(denominator)|| denominator = 0, "NaN", DIVIDE(numerator, denominator) - 1)
			RETURN  IF(difference = "NaN", Comun[Mensaje No Data] & " en " & Comun[Previous Epoca], IF(difference > 0, "+", "") & ROUND(difference * 100, 0) & "% vs " & Comun[Previous Epoca])
		lineageTag: 3fd37a45-582d-4457-880a-a2a02ae684e5

