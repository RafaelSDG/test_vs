			VAR AnoEpoca = Comun[Año Epoca] -1
			VAR Subepoca = Comun[Sub Epoca numero]
			var lytd_usuarios=
			CALCULATE(
			    SUM(App_Campus_Secciones_Agrupadas[Visualizaciones]),
			    FILTER(
			        'App_Campus_Secciones_Agrupadas',
			        'App_Campus_Secciones_Agrupadas'[Tipo Epoca] = TipoEpoca &&
			        'App_Campus_Secciones_Agrupadas'[Año] = AnoEpoca &&
			        'App_Campus_Secciones_Agrupadas'[Seccion] = "News" &&
			        (
			            (TipoEpoca = "Anual") ||
			            (TipoEpoca = "Mensual" && 'App_Campus_Secciones_Agrupadas'[Mes] = Subepoca) ||
			            (TipoEpoca = "Semestral" && 'App_Campus_Secciones_Agrupadas'[Semestre] = Subepoca)
			        )
			    )
			)
			VAR numerator = medidas_apps[Visualizaciones Secciones Avisos App by epoca]
			VAR difference = IF(ISBLANK(lytd_usuarios)|| lytd_usuarios = 0, "NaN", DIVIDE(numerator, lytd_usuarios) - 1)
			RETURN  IF(difference = "NaN", Comun[Mensaje No Data] & " en " & Comun[Previous Epoca], IF(difference > 0, "+", "") & ROUND(difference * 100, 0) & "% vs " & Comun[Previous Epoca])
		lineageTag: ef3cb66a-7de6-40cf-bfc0-a6a1d460335d

