	measure 'Tipo Epoca' = ```
			
			
			
			var is_year_selected = HASONEVALUE(Date_DIM[Año])
			var is_semester_selected = HASONEVALUE(Date_DIM[Número del Semestre])
			var is_month_selected = HASONEVALUE(Date_DIM[Nombre Mes])
			
			return IF(is_year_selected && NOT is_month_selected && NOT is_semester_selected, "Anual",
			        IF(is_year_selected && is_semester_selected && NOT is_month_selected ,"Semestral", 
			
