	measure 'Previous Epoca' = ```
			
			var is_year_selected = HASONEVALUE(Date_DIM[Año])
			var is_semester_selected = HASONEVALUE(Date_DIM[Número del Semestre])
			var is_month_selected = HASONEVALUE(Date_DIM[Nombre Mes])
			var previous_year = SELECTEDVALUE(Date_DIM[Año],0)-1
			
			return IF(is_year_selected && NOT is_month_selected && NOT is_semester_selected, CONVERT(previous_year,STRING),
			        IF(is_year_selected && is_semester_selected && NOT is_month_selected ,previous_year & "S" & SELECTEDVALUE(Date_DIM[Número del Semestre], 0), 
			
			        IF(is_year_selected && is_month_selected, previous_year & "M" & SELECTEDVALUE(Date_DIM[Número del Mes], 0), 0)))
			
