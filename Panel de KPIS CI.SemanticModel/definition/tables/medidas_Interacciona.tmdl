	measure 'LYTD Visitas noticias Interacicona totales By Epoca' =
			
			VAR TipoEpoca = Comun[Tipo Epoca]
			VAR AnoEpoca = Comun[Año Epoca] -1
			VAR Subepoca = Comun[Sub Epoca numero]
			
			VAR _max1 = MAXX(ALLSELECTED('Date_DIM'), [Date])
			VAR _max = DATE(YEAR(_max1) - 1, MONTH(_max1), DAY(_max1))
			VAR _min1 = MINX(ALLSELECTED('Date_DIM'), [Date])
			VAR _min = DATE(YEAR(_min1) - 1, MONTH(_min1), DAY(_min1))
			
			VAR Calculo =
			CALCULATE(
			    SUM(Interacciona_Noticias_Hecho[Visitas]),
			    //ALL(),
			    FILTER(ALL(Date_DIM), Date_DIM[Date]<= _max && Date_DIM[Date]>=_min),
			
			    FILTER(
			        ALL('interacciona_Noticias_Hecho'),
			        'interacciona_Noticias_Hecho'[Tipo Epoca] = TipoEpoca &&
			        'interacciona_Noticias_Hecho'[Año] = AnoEpoca &&
			        (
			            (TipoEpoca = "Anual") ||
