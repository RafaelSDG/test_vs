	measure 'Distinct Count Noticias Visitadas Inter by epoca' = ```
			
			
			VAR TipoEpoca = Comun[Tipo Epoca]
			VAR AnoEpoca = Comun[Año Epoca]
			VAR Subepoca = Comun[Sub Epoca numero]
			
			VAR Tabla = 
			SUMMARIZE(
			    'interacciona_Noticias_Hecho', 
			    'interacciona_Noticias_Hecho'[interacciona_noticia_hecho_id],
			    'interacciona_Noticias_Hecho'[Tipo Epoca],
			    'interacciona_Noticias_Hecho'[Año],
			    'interacciona_Noticias_Hecho'[Mes],
			    'interacciona_Noticias_Hecho'[Semestre],
			    "FLG",
			
			        IF(
			            TipoEpoca = "Anual" &&
			            'interacciona_Noticias_Hecho'[Tipo Epoca] = TipoEpoca &&
			            'interacciona_Noticias_Hecho'[Año] = AnoEpoca,
			            TRUE(),
			            IF(
			                TipoEpoca = "Mensual" &&
			                'interacciona_Noticias_Hecho'[Tipo Epoca] = TipoEpoca &&
			                'interacciona_Noticias_Hecho'[Año] = AnoEpoca &&
			                'interacciona_Noticias_Hecho'[Mes] = Subepoca,
			                TRUE(),
			                IF(
			                    TipoEpoca = "Semestral" &&
			                    'interacciona_Noticias_Hecho'[Tipo Epoca] = TipoEpoca &&
			                    'interacciona_Noticias_Hecho'[Año] = AnoEpoca &&
			                    'interacciona_Noticias_Hecho'[Semestre] = Subepoca,
			                    TRUE(),
			                    FALSE()
			                )
			            )
			        )
			    
			)
			
			
			VAR Tabla_filtrada = FILTER(Tabla, [FLG])
			
			VAR Filtro = FILTER('interacciona_Noticias_Hecho', 'interacciona_Noticias_Hecho'[interacciona_noticia_hecho_id] IN DISTINCT(SELECTCOLUMNS(Tabla_filtrada, "interacciona_noticia_hecho_id", [interacciona_noticia_hecho_id])))
			
			VAR Calculo = CALCULATE(DISTINCTCOUNT('interacciona_Noticias_Hecho'[interacciona_noticia_hecho_id]), Filtro)
			
			return IF(ISBLANK(Calculo),0,Calculo)
			```
