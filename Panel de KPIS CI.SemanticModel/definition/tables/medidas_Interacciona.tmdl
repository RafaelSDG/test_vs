	measure 'Usuarios Unicos Interacciona' = ```
			
			VAR TipoEpoca = Comun[Tipo Epoca]
			VAR AnoEpoca = Comun[Año Epoca]
			VAR Subepoca = Comun[Sub Epoca numero]
			
			VAR Calculo =
			CALCULATE(
			    SUM(Interacciona_Accesos_Hecho[usuarios unicos]),
			    FILTER(
			        'Interacciona_Accesos_Hecho',
			        'Interacciona_Accesos_Hecho'[Tipo Epoca] = TipoEpoca &&
			        'Interacciona_Accesos_Hecho'[Año] = AnoEpoca &&
			        (
			            (TipoEpoca = "Anual") ||
			            (TipoEpoca = "Mensual" && 'Interacciona_Accesos_Hecho'[Mes] = Subepoca) ||
			            (TipoEpoca = "Semestral" && 'Interacciona_Accesos_Hecho'[Semestre] = Subepoca)
			        )
			    )
			)
			
			RETURN IF(ISBLANK(Calculo), 0, Calculo)
			
			```
