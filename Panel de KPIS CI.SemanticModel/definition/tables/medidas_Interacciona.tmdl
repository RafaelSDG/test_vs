			    FILTER(
			        ALL('interacciona_Noticias_Hecho'),
			        'interacciona_Noticias_Hecho'[Tipo Epoca] = TipoEpoca &&
			        'interacciona_Noticias_Hecho'[Año] = AnoEpoca &&
			        (
			            (TipoEpoca = "Anual") ||
			            (TipoEpoca = "Mensual" && 'interacciona_Noticias_Hecho'[Mes] = Subepoca) ||
			            (TipoEpoca = "Semestral" && 'interacciona_Noticias_Hecho'[Semestre] = Subepoca)
			        )
			    ))
			
			
			RETURN IF(ISBLANK(Calculo), 0, Calculo)
